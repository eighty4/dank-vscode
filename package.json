{
    "publisher": "eighty4tech",
    "name": "dank-vscode",
    "displayName": "DANK",
    "version": "0.0.1",
    "private": true,
    "author": "Adam McKee Bennett <adam.be.g84d@gmail.com>",
    "repository": "https://github.com/eighty4/dank-vscode",
    "license": "MIT",
    "type": "module",
    "description": "Support for DANK website development with @eighty4/dank",
    "packageManager": "pnpm@10.28.0",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/dank.config.{js,ts}"
    ],
    "contributes": {
        "commands": []
    },
    "main": "./lib_js/extension.js",
    "scripts": {
        "build": "node esbuild.js",
        "build:release": "tsc && node esbuild.js --production",
        "fmt": "prettier --write .",
        "fmtcheck": "prettier --check .",
        "typecheck": "tsc",
        "vscode:prepublish": "pnpm build:release"
    },
    "devDependencies": {
        "@types/node": "22.x",
        "@types/vscode": "^1.74.0",
        "esbuild": "^0.27.2",
        "prettier": "^3.7.4",
        "typescript": "^5.9.3"
    }
}
